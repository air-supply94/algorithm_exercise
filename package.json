  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.19",
    "eslint": "7.15.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "zhouhaifei-code-style": "2.5.1",
    "zhouhaifei-test": "2.2.0"
  },
  "scripts": {
    "test": "zhouhaifei-test react",
    "lint-staged": "lint-staged --allow-empty",
    "lint:js": "eslint --fix --ext .js,.ts ./src",
    "prettier": "prettier -c --write \"**/*.{md,html,json}\""
  },
  "engines": {
    "node": ">=12.13.1"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "npm run lint:js",
      "git add ."
    ],
    "**/*.{md,html,json}": [
      "prettier --write",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
